function level:_box_on_tgt_at(
 x,y,game
)
 for box in all(game.boxes) do
  if (
   box:is_at(x,y)
   and box:on_tgt(self)
  ) then
   return true
  end
 end

 return false
end

function level:is_done(game)
 local old_cnt=game.box_cnt
 game.box_cnt=0

 for box in all(game.boxes) do
  if box:on_tgt(self) then
   game.box_cnt+=1
  end
 end

 if game.box_cnt>old_cnt then
  sfx(3)
 end

 return game.box_cnt==#game.boxes
end

